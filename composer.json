{
  "name": "lensa-wicara/snap-bi",
  "description": "Standart National Open API Payments wrapper",
  "type": "package",
  "authors": [
    {
      "name": "Syakirin Amin",
      "email": "akrinmin@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0",
    "guzzlehttp/guzzle": "7.9.x-dev"
  },
  "require-dev": {
    "laravel/pint": "^1.1",
    "nunomaduro/collision": "^7.8",
    "orchestra/testbench": "dev-develop",
    "phpseclib/phpseclib": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "LensaWicara\\SnapBI\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LensaWicara\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@clear",
      "@prepare"
    ],
    "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
    "prepare": "@php vendor/bin/testbench package:discover --ansi",
    "build": "@php vendor/bin/testbench workbench:build --ansi",
    "serve": [
      "Composer\\Config::disableProcessTimeout",
      "@build",
      "@php vendor/bin/testbench serve"
    ],
    "fix": [
      "pint --preset laravel"
    ],
    "test": [
      "testbench package:test --ansi"
    ],
    "testbench": [
      "testbench"
    ]
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "LensaWicara\\SnapBI\\ServiceProvider"
      ]
    }
  }
}
